// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model warehouse {
  id          Int    @id @default(autoincrement())
  name        String
  description String
}

model products {
  id          Int      @id @default(autoincrement())
  date        DateTime @default(now())
  name        String
  description String
  quantity    Float
  buyPrice    Float
  sellPrice   Float
}
